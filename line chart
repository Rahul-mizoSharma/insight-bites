import matplotlib.pyplot as plt
import numpy as np

# Sample data
x = np.arange(1, 11)  # X-axis values
y1 = np.random.randint(100, 151, size=10)  # Y-axis values for line 1
y2 = np.random.randint(100, 151, size=10)  # Y-axis values for line 2

# Create line chart
plt.plot(x, y1, label='Data Set 1', marker='o')
plt.plot(x, y2, label='Data Set 2', marker='s')

# Adding titles and labels
plt.title('Line Chart Example')
plt.xlabel('X-axis')
plt.ylabel('Y-axis')
plt.legend()

# Show the plot
plt.show()
